/*播放列表菜单bar*/
QMenuBar#menuBar{
    spacing: 9px;
}
QMenuBar#menuBar::item{
    background: transparent;}
QMenuBar#menuBar::item:selected{        /* 跳动效果 */
    padding: 2px,5px,6px,3px;
}
QMenu:selected{
    background-color: rgb(115,80,160);     /* rgb(15,35,45) */
}
QMenu{
    background-color: rgb(60,65,105);
    color: rgb(150,200,250);            /* rgb(185,120,235) */
    font:  12px   "Tahoma";
}
QMenu::separator{
    height: 1px;
    background: lightblue;
}
QMenu::item:disabled
{
    color:  #bbbbbb;
}

/*************** toolTip     ******************************/
QToolTip{
    color:  rgb(90, 71, 82);
    background-color: rgb(200,220,255);
    opacity:    200;
    border-radius: 6px;
    font: 11px "Tahoma";
    padding: 1px;
    border-color: darkslategray;
    border-width: 1px;
    border-style: solid;
}


/* 播放列表滑块************************************* */

QScrollBar:horizontal {
    background: transparent;
    height: 9px;
    margin: 0px, 0px, 0px, 0px;
    padding-left:   9px;            /*必须预留位置给箭头*/
    padding-right:    9px;
}

/* 水平滑块 */
QScrollBar::handle:horizontal {
    height: 9px;
    border-image:  url(./res/skin1/hbar0.gif);
    min-width:    20;
}
QScrollBar::handle:horizontal:hover{
    height: 9px;
    border-image:  url(./res/skin1/hbar1.gif);
    min-width:    20;
}
QScrollBar::handle:horizontal:pressed{
    height: 9px;
    border-image:  url(./res/skin1/hbar2.gif);
    min-width:    20;
}
/* 左箭头 */
QScrollBar::sub-line:horizontal{
    height:    9px;
    width:     9px;
    border-image:  url(./res/skin1/leftarrow0.gif);
    subcontrol-position:   left;
}
QScrollBar::sub-line:horizontal:hover{
    border-image:   url(./res/skin1/leftarrow1.gif);
}
QScrollBar::sub-line:horizontal:pressed{
    border-image:   url(./res/skin1/leftarrow2.gif);
}
/*右箭头*/
QScrollBar::add-line:horizontal{
    height:    9px;
    width:     9px;
    border-image:  url(./res/skin1/rightarrow0.gif);
    subcontrol-position:   right;
}
QScrollBar::add-line:horizontal:hover{
    border-image:   url(./res/skin1/rightarrow1.gif);
}
QScrollBar::add-line:horizontal:pressed{
    border-image:   url(./res/skin1/rightarrow2.gif);
}
/* 滚动槽 */
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal{
    border-image:   url(./res/skin1/hscrollbar_slot.bmp);
}

/****** 竖直滑块 ******/
QScrollBar:vertical {
    background: transparent;
    width: 9px;
    margin: 0px, 0px, 0px, 0px;
    padding-top:   9px;            /*必须预留位置给箭头*/
    padding-bottom:    9px;
}
QScrollBar::handle:vertical {
    width: 9px;
    border-image:  url(./res/skin1/bar0.gif);
    min-height:    20;
}
QScrollBar::handle:vertical:hover{
    width: 9px;
    border-image:  url(./res/skin1/bar1.gif);
    min-height:    20;
}
QScrollBar::handle:vertical:pressed{
    width: 9px;
    border-image:  url(./res/skin1/bar2.gif);
    min-height:    20;
}


/* 下箭头 */
QScrollBar::add-line:vertical{
    height:    9px;
    width:     9px;
    border-image:  url(./res/skin1/downarrow0.gif);
    subcontrol-position:   bottom;
}
QScrollBar::add-line:vertical:hover{
    border-image:   url(./res/skin1/downarrow1.gif);
}
QScrollBar::add-line:vertical:pressed{
    border-image:   url(./res/skin1/downarrow2.gif);
}

/* 上箭头 */
QScrollBar::sub-line:vertical {
    height:    9px;
    width:     9px;
    border-image:  url(./res/skin1/uparrow0.gif);
    subcontrol-position:   top;
}
QScrollBar::sub-line:vertical:hover{
    border-image:   url(./res/skin1/uparrow1.gif);
}
QScrollBar::sub-line:vertical:pressed{
    border-image:   url(./res/skin1/uparrow2.gif);
}

/* 滚动槽 */
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical{
    border-image:   url(./res/skin1/scrollbar_slot.bmp);
}


/* 分割条，怎么消掉残余的线条，本来就没有，只是QStackWidget当前page有个主部件 */
/* 然后主部件里面进行的布局，布局貌似是不能铺满窗口的！？！？*/
/* 解决办法就是一个page只使用一个部件，自动就不会有间隙的了*/
QSplitter#spliter::handle:horizontal{
    border-image:   url(./res/skin1/div.bmp);
}


/* 在 播放列表、网络搜索窗口 共用共用共用共用共用 *****************************/
QListWidget{
    font:   13px "Tahoma";
    color:  #8384b8;
    background-color:  qlineargradient(x1:0, y1:0, x2:0, y2:1,
                        stop:0 #272a46, stop: 0.4 #474a66, stop:1 #373a56);/*#272a46;*/

 /*   spacing:    3px;      不支持该部件*/
    border:     0px 0px 0px 0px;        /*去掉在分裂器周围的界限*/
}

QSplitter{     /*删除列表时可能会显示最后面的没有填充的白色*/
    background-color:  qlineargradient(x1:0, y1:0, x2:0, y2:1,
                        stop:0 #272a46, stop: 0.4 #474a66, stop:1 #373a56);
}

/*列表里面的项目  播放列表、网络搜索窗口  悬停*/
QListWidget::item:hover{
    color:  white;              /*字体*/
    background-color: #5f2485;
    border-radius:  3px;
    border: 1px outset white;
}

/*被选择 播放列表、网络搜索窗口 */
QListWidget::item:selected{
    color:  #ef8bfb;              /*字体*/
    background-color: #373a56;
}

/*被选择 + 悬停 QSplitter#spliter */
QListWidget::item:selected:hover{
    color:  #66ff77;              /*字体*/
    background-color: #7f24e5;
}

/************** QTableWidget 歌曲列表 *************************/

/*表格里面*/
QTableWidget{
    font:   13px "Tahoma";
    color:  #8384b8;
    background-color:  qlineargradient(x1:0, y1:0, x2:0, y2:1,
                        stop:0 #272a46, stop: 0.4 #474a66, stop:1 #373a56);/*#272a46;*/
    border:     0px 0px 0px 0px;        /*去掉在分裂器周围的界限*/
}

/*悬停*/
QStackedWidget#playListStackWidget QTableWidget::item:hover{
    color:  white;              /*字体*/
    background-color: #5f2485;
    border-radius:  3px;
    border: 1px outset white;
}

/*被选择*/
QTableWidget::item:selected{
    color:  #ef8bfb;              /*字体*/
    background-color: #373a56;
}

/*被选择 + 悬停、排除下载管理里面的 tablewidget*/
QStackedWidget#playListStackWidget QTableWidget::item:selected:hover{
    color:  #66ff77;              /*字体*/
    background-color: #7f24e5;
}

/*******************表格的表头的每个头************** */
QHeaderView::section:vertical{       /*表头里的每个标签*/
    font:   13px "Tahoma";
    color:  #8384b8;
    /*background-color:    #373a56;*/
    background: transparent;
    border: 0px 0px 0px 0px;        /*本来是可以伸缩的，去掉之后就不会伸缩了*/
    margin: 0px 0px 0px 0px;
    padding:    2px 4px 2px 0px;    /*5px 为了拉开序列号与歌曲名之间距离，会影响表头设置的对齐方式*/
}

QHeaderView:vertical{
    background-color:  qlineargradient(x1:0, y1:0, x2:0, y2:1,
                        stop:0 #272a46, stop: 0.4 #474a68, stop:1 #373a56);
}

/*********** 网络音乐窗口的 treeWidget 水平表头 ************/
QHeaderView::section:horizontal{       /*表头里的每个标签*/
    font:   13px "Tahoma";
    color:  #8384b8;
    background-color:  qlineargradient(x1:0, y1:0, x2:1, y2:1,
                        stop:0 #272a46, stop: 1 #404aa6 );
    border: 1px solid #24cdd8;
}



