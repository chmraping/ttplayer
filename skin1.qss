/* 主窗口 */
QWidget#MyWidget{
background-image: url(./res/skin1/player_skin.png);
}

/********** 迷你主窗口 ***********************/

QWidget#miniMainWnd
{
    border-image:   url(./res/skin1/mini-player.bmp);
}

/******** 窗口歌词里面的 QLabel ********/
QLabel#lrcLab{
    background-color:   qlineargradient(x1:0, y1:0, x2:0, y2:1,
                            stop:0 #272a46, stop: 0.4 #474a66, stop:1 #373a56);/*#272a46;*/
    border:     1px white solid outset;
    border-radius:  4px;
}


/************** 迷你窗口歌词 *******************/
QWidget#miniLrcWnd
{
    background:   #272a46;
}

/******** 网络音乐窗口 *********/
QWidget#brower{
    background-color:  qlineargradient(x1:0, y1:0, x2:0, y2:1,
                            stop:0 #8e75f9, stop:1 #675ac6);
    /* qlineargradient(x1:0, y1:0, x2:1, y2:1,
                            stop:0 #353c86, stop: 0.4 #272a46, stop:1 #373a56);*/
}

/******* 搜索输入编辑框 ********/
QLineEdit#browerLineEdit{
    background-color:   #272a46;
    color:      #00ff3c;
    border: 1px outset light solid ;
    border-radius:  3px;
}

/****** 音乐窗口里面的 treeWidget *******/
QTreeWidget{
    background-color:  qlineargradient(x1:0, y1:0, x2:0, y2:1,
                          stop:0 #272a46, stop: 0.4 #474a66, stop:1 #373a56);
    color:  #8e75f9;
    border: 1px inset #7d17f0 solid;
    border-radius:  3px;
    font:   12px "楷体";
}
QTreeWidget::item:selected{
    background-color:   #5e4071;
    color:      #8e75f9;
}

/******** 下载任务里面的进度条 *******/
QProgress{
    border: 15px;
    border-radius: 5px;
    height: 10px;
}
QProgressBar::chunk {
    background-color: #05B8CC;
    width: 10px;
}

/************* 下载窗口内的工具栏 **************/
QToolBar{
    background-color:   #714fd3;
    spacing:    5px;
    border-left: 1px inset #ceced0 solid;
    border-top: 1px inset #ceced0 solid;
    border-right: 1px outset #4c4855 solid;
    border-bottom: 1px outset #4c4855 solid;
    border-radius:  2px;
}

